@model SelectList

@{
    ViewBag.Title = "List Images By User";
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm("DoListByUser", "Image", FormMethod.Get))
{
    <fieldset>
        <legend>Choose a user</legend>
        <p>
            User: @Html.DropDownList("Id", Model)
        </p>
        <p>
            <input type="submit" value="Query" />
        </p>
    </fieldset>

    <p>@ViewBag.Message</p>

}