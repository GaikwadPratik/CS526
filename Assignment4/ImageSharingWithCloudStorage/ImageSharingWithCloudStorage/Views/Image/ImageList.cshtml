@model SelectList
@{
    ViewBag.Title = "Image Viewing Activity";
}
@using (Html.BeginForm("ImageViews", "Image", FormMethod.Post))
{
    <h2>@ViewBag.Title</h2>

    <legend>Select a Date</legend>
    <fieldset>
        <p>
            @Html.DropDownList("dateTaken", (List<SelectListItem>)ViewBag.dateList)
        </p>
        <form>
            <input type="submit" value="List" />
        </form>
    </fieldset>
}