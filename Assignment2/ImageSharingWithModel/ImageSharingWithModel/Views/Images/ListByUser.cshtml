@model SelectList
@{
    ViewBag.Title = "List images by User";
}

<h2>@ViewBag.Title</h2>
@using (Html.BeginForm("DoListByUser", "Images", FormMethod.Get))
{
    <fieldset>
        <legend>Choose a User</legend>
        <p>
            User: @Html.DropDownList("Id", Model)
        </p>

        <p>
            <input type="submit" value="List" />
        </p>
    </fieldset>

    @ViewBag.Message
}

